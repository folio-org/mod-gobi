        "CREATE_INVENTORY",
        "CURRENCY",
        "DATE_ORDERED",
        "DESCRIPTION",
        "DISCOUNT",
        "DISCOUNT_TYPE",
        "DONOR",
        "EDITION", 
        "ENCUMBERANCE",
        "EXPECTED_ACTIVATION",
        "EXPECTED_RECEIPT_DATE",
        "FUND_ID",
        "FUND_CODE",
        "FUND_PERCENTAGE",
        "LICENSE_CODE",
        "LICENSE_DESCRIPTION",
        "LICENSE_REFERENCE",
        "LIST_UNIT_PRICE",
        "LIST_UNIT_PRICE_ELECTRONIC",
        "LOCATION",
        "MANUAL_PO",
        "MATERIAL_SUPPLIER",
