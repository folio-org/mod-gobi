        "ORDER_TYPE",
        "PO_LINE_DESCRIPTION",
        "PO_LINE_ESTIMATED_PRICE",
        "PO_LINE_ORDER_FORMAT",
        "PO_LINE_PAYMENT_STATUS",
        "PO_LINE_RECEIPT_STATUS",
        "PRODUCT_ID",
        "PRODUCT_ID_TYPE",
        "PUBLICATION_DATE",
        "PUBLISHER",
        "PURCHASE_ORDER_ID",
        "QUANTITY_ELECTRONIC",
        "QUANTITY_PHYSICAL",
        "RECEIPT_DATE",
        "RECEIPT_DUE",
        "RECEIVING_NOTE",
        "RENEWAL_CYCLE",
        "RENEWAL_DATE",
        "RENEWAL_INTERVAL",
        "RENEWAL_MANUAL",
        "RENEWAL_REVIEW_PERIOD",
        "REPORTING_CODE",
        "REPORTING_DESCRIPTION",
        "REQUESTER",
