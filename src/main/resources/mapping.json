				"PO_LINE_WORKFLOW_STATUS",
				"PRODUCT_ID",
				"PRODUCT_ID_TYPE",
				"PUBLICATION_DATE",
				"PUBLISHER",
				"PURCHASE_ORDER_ID",
				"QUANTITY_ORDERED_ELECTRONIC",
				"QUANTITY_ORDERED_PHYSICAL",
